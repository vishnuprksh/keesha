# Optional: Set to 'true' to use Firebase emulators in development
# REACT_APP_USE_FIRESTORE_EMULATOR=true
# REACT_APP_USE_AUTH_EMULATOR=true

# For the standalone test script (check_gemini_api.js)
GEMINI_API_KEY=your_gemini_api_key_here
